{
  "siteName": "Nxus App",
  "host": "localhost",
  "baseUrl": "localhost:3001",
  "storage": {
    "adapters": {
      "default": "waterline-sqlite3"
    },
    "connections": {
      "default": {
        "adapter": "default"
      }
    }
  },
  "sendgrid": {
    "disabled": true
  },
  "worker_queue": {
    "redis_url": "redis://localhost:6379"
  },
  "client_js": {
    "babel": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        "transform-function-bind",
        "transform-object-rest-spread"
      ]
    }
  }
}
